<?xml version="1.0" ?>
<sdf version="1.4">
	<world name="ground_plane">
		<!-- A global light source -->

		<include>
			<uri>model://ground_plane</uri>
		</include>

		<include>
			<uri>model://sun</uri>
		</include>

		<scene>
			<sky>
				<clouds>
					<speed>12</speed>
				</clouds>
			</sky>
		</scene>

		<!-- World Plugin Example
		 <plugin name="oars_plugin" filename="liboars_plugin.so"/>
		-->
	</world>
</sdf>
